<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.ouroom.web.brddt.BrdDetailMapper">
<select id="item_seq" resultType="com.ouroom.web.brddt.Item">
	SELECT 
	TITLE,PRICE,DELIVERY,DISCOUNT,PHOTO,SALE_CNT,STORE_CNT
	FROM ITEM
	WHERE SEQ LIKE #{seq};
</select>
</mapper>